{
  "kind": "collectionType",
  "collectionName": "feeds",
  "info": {
    "singularName": "feed",
    "pluralName": "feeds",
    "displayName": "Feed"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "public": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "allowed_users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "feeds"
    }
  }
}
