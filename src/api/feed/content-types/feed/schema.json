{
  "kind": "collectionType",
  "collectionName": "feeds",
  "info": {
    "singularName": "feed",
    "pluralName": "feeds",
    "displayName": "Feed",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "public": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "allowed_users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "feeds"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "episodes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::episode.episode",
      "inversedBy": "feeds"
    },
    "data": {
      "type": "text"
    },
    "email": {
      "type": "email",
      "default": "changeme@unknown.com",
      "required": true
    },
    "copyright": {
      "type": "string",
      "default": "All rights reserved",
      "required": true
    },
    "owner": {
      "type": "string",
      "required": true,
      "default": "Some Owner"
    }
  }
}
