{
  "kind": "collectionType",
  "collectionName": "episodes",
  "info": {
    "singularName": "episode",
    "pluralName": "episodes",
      "displayName": "Episode",
      "description": ""
  },
  "options": {
      "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
      "audio": {
          "type": "media",
          "multiple": false,
          "required": true,
      "allowedTypes": [
        "files",
        "audios"
      ]
    },
    "feeds": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::feed.feed",
      "mappedBy": "episodes"
    },
      "cover": {
          "type": "media",
          "multiple": false,
          "required": true,
          "allowedTypes": [
              "images",
              "files"
          ]
      },
      "guid": {
          "type": "uid"
      },
      "data": {
          "type": "text"
      },
      "description": {
          "type": "text"
      },
      "duration": {
          "type": "integer",
          "min": 1,
          "required": true
      },
      "releasedAt": {
          "type": "datetime",
          "required": true
      },
      "draft": {
          "type": "boolean",
          "default": false,
          "required": true
    }
  }
}
